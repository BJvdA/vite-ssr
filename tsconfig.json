{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "ESNext",
    "target": "es2019",
    "lib": [ "ESNext", "DOM" ],
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "stripInternal": true
  },
  "include": [
    "./src",
  ]
}