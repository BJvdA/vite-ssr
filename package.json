{
  "name": "vite-ssr",
  "version": "0.6.8",
  "private": true,
  "description": "Vite utility for server side rendering",
  "main": "./entry-server.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "copy": "cp README.md LICENSE dist/ && sed -e '/\"private\"/d' package.json > dist/package.json",
    "build": "yarn clean && yarn copy && cd src && yarn tsc",
    "dev": "rm -rf node_modules/vite-ssr && mkdir node_modules/vite-ssr && cp package.json ./node_modules/vite-ssr && cd src && yarn tsc --watch --incremental --outDir ./node_modules/vite-ssr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frandiox/vite-ssr.git"
  },
  "keywords": [
    "vite",
    "ssr",
    "vue",
    "react"
  ],
  "author": "Fran Dios",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frandiox/vite-ssr/issues"
  },
  "homepage": "https://github.com/frandiox/vite-ssr#readme",
  "bin": "./cli.js",
  "types": "./index.d.ts",
  "workspaces": [
    "examples/vue",
    "examples/react"
  ],
  "peerDependencies": {
    "@vueuse/head": "^0.5.0",
    "react": "^16.8 || ^17",
    "react-dom": "^16 || ^17",
    "react-helmet-async": "^1.0.0",
    "react-router-dom": "^5.0.0",
    "vite": "^2.0.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@vueuse/head": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "vue-router": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "react-helmet-async": {
      "optional": true
    },
    "react-router-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/plugin-replace": "^2.3.3",
    "@vue/server-renderer": "^3.0.0",
    "connect": "^3.7.0",
    "node-fetch": "^2.6.1",
    "react-ssr-prepass": "^1.4.0"
  },
  "devDependencies": {
    "tsc": "^1.20150623.0",
    "typescript": "^4.2.3"
  }
}