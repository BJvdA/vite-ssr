{
  "name": "example",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --port 1337 --open --force",
    "dev:ssr": "vite-ssr --port 1337 --open --force",
    "build": "rm -rf dist && vite-ssr build",
    "refresh": "rm -rf node_modules/vite-ssr && cp -r ../../core node_modules/vite-ssr && cp ../../core/cli.js node_modules/.bin/vite-ssr",
    "serve:node": "node ../node-server/index vue"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "vite": "^2.0.0",
    "vite-ssr": "file:../../core",
    "vue": "^3.0.5",
    "vue-router": "^4.0.0"
  }
}